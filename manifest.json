{

  "manifest_version": 2,
  "name": "Random Link Picker",
  "version": "1.0",

  "description": "A Simple random link picker",
  "icons": {
    "48": "randomIcon.png"
  },
  
  "options_ui": {
      "page": "options.html"
   },

  "permissions": [
    "activeTab",
	"bookmarks"
  ],

  "browser_action": {
    "default_icon": "randomIcon.png",
    "default_title": "Random Link Picker",
	"default_popup": "ffpopup.html"
  }



}